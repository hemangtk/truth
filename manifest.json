{
    "manifest_version": 3,
    "name": "Google Fake Review Identifier",
    "version": "1.0",
    "description": "Identifies potentially fake reviews on Google",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.google.com/*"],
        "js": ["content-script.js"],
        "css": ["content-styles.css"]
      }
    ],
    "icons": {
      "16": "icon16.jpg",
      "48": "icon48.jpg",
      "128": "icon128.jpg"
    }
  }
  
  